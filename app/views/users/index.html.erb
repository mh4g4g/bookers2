<div class="container">
<div class="row">
<%= render 'books/list', book: @book %>
<div class="col-md-8 offset-md-1">

  <h2>Users</h2>
  <% @users.each do |user| %>
  
    <p>image</p>
    <%= image_tag user.get_profile_image(100,100) %>
    <p>name</p>
    <%= user.name %>
    <%= link_to "Show", user_path(user.id) %>
  <% end %>

</div>
</div>
</div>